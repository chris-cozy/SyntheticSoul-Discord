TOKEN=replace_with_discord_bot_token
TEST_SERVER=replace_with_guild_id
ENABLE_USER_INSTALLS=true

# API call target from inside Docker container:
SYNTHETIC_SOUL_API_BASE_URL=http://api:8000/v1

# Must match the API stack shared network.
SHARED_DOCKER_NETWORK=synthetic-soul-shared

# Optional tuning
SYNTHETIC_SOUL_HTTP_TIMEOUT_MS=30000
SYNTHETIC_SOUL_JOB_MAX_POLLS=50
SYNTHETIC_SOUL_JOB_BASE_POLL_MS=1200
